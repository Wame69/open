            // –ù–æ–≤—ã–µ –º–∞—Ä—à—Ä—É—Ç—ã
            new TransportRoute("–°–∞–º–æ–ª–µ—Ç", "–†–æ—Å—Ç–æ–≤-–Ω–∞-–î–æ–Ω—É", "–í–ª–∞–¥–∏–≤–æ—Å—Ç–æ–∫", false, "09:00", "18:00", "15000 —Ä—É–±.",
                new List<string> { "–°–∞–º–∞—Ä–∞", "–ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫" }), // –ü–µ—Ä–µ—Å–∞–¥–∫–∏ –≤ –°–∞–º–∞—Ä–µ –∏ –ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫–µ
