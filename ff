        cities[19].Places.Add(new PlaceOfInterest("Павлодарская набережная", "Набережная в Павлодаре", "Чебуреки"));
        cities[20].Places.Add(new PlaceOfInterest("Актобе", "Город в Казахстане", "Бешбармак"));

        cities[21].Places.Add(new PlaceOfInterest("Тяньаньмэнь", "Площадь в Пекине", "Пекинская утка"));
        cities[22].Places.Add(new PlaceOfInterest("Шанхайская набережная", "Набережная в Шанхае", "Дим-сам"));
        cities[23].Places.Add(new PlaceOfInterest("Гуанчжоуский храм", "Древний храм в Гуанчжоу", "Лапша"));
        cities[24].Places.Add(new PlaceOfInterest("Чэнду Ли Ху", "Природный парк в Чэнду", "Хот-пот"));
        cities[25].Places.Add(new PlaceOfInterest("Сианьский музей", "Музей в Сиане", "Мечта"));
        cities[26].Places.Add(new PlaceOfInterest("Шэньчжэньский парк", "Городской парк в Шэньчжэне", "Шашлык"));

        return cities;
