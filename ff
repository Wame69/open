                new List<string> { "–ê—Å—Ç–∞–Ω–∞" }), // –ü–µ—Ä–µ—Å–∞–¥–∫–∞ –≤ –ê—Å—Ç–∞–Ω–µ
            new TransportRoute("–ü–æ–µ–∑–¥", "–®–∞–Ω—Ö–∞–π", "–ü–µ–∫–∏–Ω", false, "07:00", "15:00", "3500 —Ä—É–±.",
                new List<string> { "–ì—É–∞–Ω—á–∂–æ—É", "–ß—ç–Ω–¥—É" }), // –ü–µ—Ä–µ—Å–∞–¥–∫–∞ –≤ –ì—É–∞–Ω—á–∂–æ—É –∏ –ß—ç–Ω–¥—É
            new TransportRoute("–°–∞–º–æ–ª–µ—Ç", "–ú–æ—Å–∫–≤–∞", "–®–∞–Ω—Ö–∞–π", false, "15:00", "23:00", "10000 —Ä—É–±.",
                new List<string> { "–ù–∏–∂–Ω–∏–π –ù–æ–≤–≥–æ—Ä–æ–¥", "–¢—É—Ä–∫–µ—Å—Ç–∞–Ω" }), // –ü–µ—Ä–µ—Å–∞–¥–∫–∏ –≤ –ù–∏–∂–Ω–µ–º –ù–æ–≤–≥–æ—Ä–æ–¥–µ –∏ –¢—É—Ä–∫–µ—Å—Ç–∞–Ω–µ
