            new TransportRoute("Самолет", "Красноярск", "Москва", false, "12:00", "20:00", "7000 руб.",
                new List<string> { "Казань" }), // Пересадка в Казани

            // Добавление маршрута Москва - Минск
            new TransportRoute("Поезд", "Москва", "Минск", true, "09:00", "14:00", "2200 руб."),

            // Добавленные маршруты
