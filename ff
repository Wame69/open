            new City("Витебск", "Беларусь"),
            new City("Брест", "Беларусь"),
            new City("Гродно", "Беларусь"),

            // Пример городов и интересных мест (Казахстан)
            new City("Астана", "Казахстан"),
            new City("Алматы", "Казахстан"),
            new City("Шымкент", "Казахстан"),
            new City("Павлодар", "Казахстан"),
            new City("Караганда", "Казахстан"),
            new City("Актобе", "Казахстан"),

            // Пример городов и интересных мест (Китай)
            new City("Пекин", "Китай"),
            new City("Шанхай", "Китай"),
            new City("Гуанчжоу", "Китай"),
            new City("Чэнду", "Китай"),
