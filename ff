            new TransportRoute("Поезд", "Алматы", "Астана", true, "13:00", "15:00", "1500 руб."),
            new TransportRoute("Самолет", "Санкт-Петербург", "Екатеринбург", false, "09:00", "14:00", "4000 руб.",
