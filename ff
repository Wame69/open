public static class DataProvider
{
    public static List<City> GetCities()
    {
        var cities = new List<City>
        {
            // –ü—Ä–∏–º–µ—Ä –≥–æ—Ä–æ–¥–æ–≤ –∏ –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã—Ö –º–µ—Å—Ç (–†–æ—Å—Å–∏—è)
            new City("–ú–æ—Å–∫–≤–∞", "–†–æ—Å—Å–∏—è"),
            new City("–°–∞–Ω–∫—Ç-–ü–µ—Ç–µ—Ä–±—É—Ä–≥", "–†–æ—Å—Å–∏—è"),
            new City("–ù–æ–≤–æ—Å–∏–±–∏—Ä—Å–∫", "–†–æ—Å—Å–∏—è"),
            new City("–ï–∫–∞—Ç–µ—Ä–∏–Ω–±—É—Ä–≥", "–†–æ—Å—Å–∏—è"),
            new City("–ö–∞–∑–∞–Ω—å", "–†–æ—Å—Å–∏—è"),
            new City("–ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫", "–†–æ—Å—Å–∏—è"),
            new City("–£—Ñ–∞", "–†–æ—Å—Å–∏—è"),
            new City("–°–∞–º–∞—Ä–∞", "–†–æ—Å—Å–∏—è"),
            new City("–í–ª–∞–¥–∏–≤–æ—Å—Ç–æ–∫", "–†–æ—Å—Å–∏—è"),
            new City("–†–æ—Å—Ç–æ–≤-–Ω–∞-–î–æ–Ω—É", "–†–æ—Å—Å–∏—è"),

            // –ü—Ä–∏–º–µ—Ä –≥–æ—Ä–æ–¥–æ–≤ –∏ –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã—Ö –º–µ—Å—Ç (–ë–µ–ª–∞—Ä—É—Å—å)
            new City("–ú–∏–Ω—Å–∫", "–ë–µ–ª–∞—Ä—É—Å—å"),
            new City("–ì–æ–º–µ–ª—å", "–ë–µ–ª–∞—Ä—É—Å—å"),
            new City("–ú–æ–≥–∏–ª–µ–≤", "–ë–µ–ª–∞—Ä—É—Å—å"),
            new City("–í–∏—Ç–µ–±—Å–∫", "–ë–µ–ª–∞—Ä—É—Å—å"),
            new City("–ë—Ä–µ—Å—Ç", "–ë–µ–ª–∞—Ä—É—Å—å"),
            new City("–ì—Ä–æ–¥–Ω–æ", "–ë–µ–ª–∞—Ä—É—Å—å"),

            // –ü—Ä–∏–º–µ—Ä –≥–æ—Ä–æ–¥–æ–≤ –∏ –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã—Ö –º–µ—Å—Ç (–ö–∞–∑–∞—Ö—Å—Ç–∞–Ω)
            new City("–ê—Å—Ç–∞–Ω–∞", "–ö–∞–∑–∞—Ö—Å—Ç–∞–Ω"),
            new City("–ê–ª–º–∞—Ç—ã", "–ö–∞–∑–∞—Ö—Å—Ç–∞–Ω"),
            new City("–®—ã–º–∫–µ–Ω—Ç", "–ö–∞–∑–∞—Ö—Å—Ç–∞–Ω"),
            new City("–ü–∞–≤–ª–æ–¥–∞—Ä", "–ö–∞–∑–∞—Ö—Å—Ç–∞–Ω"),
            new City("–ö–∞—Ä–∞–≥–∞–Ω–¥–∞", "–ö–∞–∑–∞—Ö—Å—Ç–∞–Ω"),
            new City("–ê–∫—Ç–æ–±–µ", "–ö–∞–∑–∞—Ö—Å—Ç–∞–Ω"),

            // –ü—Ä–∏–º–µ—Ä –≥–æ—Ä–æ–¥–æ–≤ –∏ –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã—Ö –º–µ—Å—Ç (–ö–∏—Ç–∞–π)
            new City("–ü–µ–∫–∏–Ω", "–ö–∏—Ç–∞–π"),
            new City("–®–∞–Ω—Ö–∞–π", "–ö–∏—Ç–∞–π"),
            new City("–ì—É–∞–Ω—á–∂–æ—É", "–ö–∏—Ç–∞–π"),
            new City("–ß—ç–Ω–¥—É", "–ö–∏—Ç–∞–π"),
            new City("–°–∏–∞–Ω—å", "–ö–∏—Ç–∞–π"),
            new City("–®—ç–Ω—å—á–∂—ç–Ω—å", "–ö–∏—Ç–∞–π")
        };

        // –ü—Ä–∏–º–µ—Ä –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã—Ö –º–µ—Å—Ç –¥–ª—è –∫–∞–∂–¥–æ–≥–æ –≥–æ—Ä–æ–¥–∞
        cities[0].Places.Add(new PlaceOfInterest("–ö—Ä–∞—Å–Ω–∞—è –ø–ª–æ—â–∞–¥—å", "–ó–Ω–∞–º–µ–Ω–∏—Ç–∞—è –ø–ª–æ—â–∞–¥—å –≤ –ú–æ—Å–∫–≤–µ", "–ü–µ–ª—å–º–µ–Ω–∏"));
        cities[1].Places.Add(new PlaceOfInterest("–≠—Ä–º–∏—Ç–∞–∂", "–û–¥–∏–Ω –∏–∑ –∫—Ä—É–ø–Ω–µ–π—à–∏—Ö –º—É–∑–µ–µ–≤ –º–∏—Ä–∞", "–ö–≤–∞—Å"));
        cities[2].Places.Add(new PlaceOfInterest("–ë–æ—Ç–∞–Ω–∏—á–µ—Å–∫–∏–π —Å–∞–¥", "–ö—Ä–∞—Å–∏–≤—ã–π —Å–∞–¥ –≤ –ù–æ–≤–æ—Å–∏–±–∏—Ä—Å–∫–µ", "–®–∞—à–ª—ã–∫"));
        cities[3].Places.Add(new PlaceOfInterest("–•—Ä–∞–º –Ω–∞ –∫—Ä–æ–≤–∏", "–ò–∑–≤–µ—Å—Ç–Ω–∞—è —Ü–µ—Ä–∫–æ–≤—å –≤ –ï–∫–∞—Ç–µ—Ä–∏–Ω–±—É—Ä–≥–µ", "–£—Ö–∞"));
        cities[4].Places.Add(new PlaceOfInterest("–ö–∞–∑–∞–Ω—Å–∫–∏–π –∫—Ä–µ–º–ª—å", "–ò—Å—Ç–æ—Ä–∏—á–µ—Å–∫–∞—è –∫—Ä–µ–ø–æ—Å—Ç—å –≤ –ö–∞–∑–∞–Ω–∏", "–ß–µ–±—É—Ä–µ–∫"));
        cities[5].Places.Add(new PlaceOfInterest("–¶–µ–Ω—Ç—Ä–∞–ª—å–Ω—ã–π –ø–∞—Ä–∫", "–ü—Ä–µ–∫—Ä–∞—Å–Ω—ã–π –ø–∞—Ä–∫ –≤ –ö—Ä–∞—Å–Ω–æ—è—Ä—Å–∫–µ", "–©–∏"));
        cities[6].Places.Add(new PlaceOfInterest("–ò–º–∞–Ω–æ–≤ –ø–∞—Ä–∫", "–ö—Ä–∞—Å–∏–≤—ã–π –ø–∞—Ä–∫ –≤ –£—Ñ–µ", "–ú–∞–Ω—Ç—ã"));
        cities[7].Places.Add(new PlaceOfInterest("–°–∞–º–∞—Ä—Å–∫–∏–π –∑–æ–æ–ø–∞—Ä–∫", "–ó–æ–æ–ø–∞—Ä–∫ –≤ –°–∞–º–∞—Ä–µ", "–ë–æ—Ä—â"));
        cities[8].Places.Add(new PlaceOfInterest("–í–ª–∞–¥–∏–≤–æ—Å—Ç–æ–∫—Å–∫–∞—è –Ω–∞–±–µ—Ä–µ–∂–Ω–∞—è", "–ü—Ä–µ–∫—Ä–∞—Å–Ω–∞—è –Ω–∞–±–µ—Ä–µ–∂–Ω–∞—è –≤ –í–ª–∞–¥–∏–≤–æ—Å—Ç–æ–∫–µ", "–†—ã–±–Ω—ã–µ –±–ª—é–¥–∞"));
        cities[9].Places.Add(new PlaceOfInterest("–†–æ—Å—Ç–æ–≤—Å–∫–∏–π —Ü–∏—Ä–∫", "–û–¥–∏–Ω –∏–∑ –ª—É—á—à–∏—Ö —Ü–∏—Ä–∫–æ–≤ –≤ –†–æ—Å—Å–∏–∏", "–®–∞—à–ª—ã–∫–∏"));

        cities[10].Places.Add(new PlaceOfInterest("–ö—Ä–µ–º–ª—å –≤ –ú–∏–Ω—Å–∫–µ", "–°—Ç–∞—Ä–∏–Ω–Ω—ã–π –∫—Ä–µ–º–ª—å –≤ –ú–∏–Ω—Å–∫–µ", "–î—Ä–∞–Ω–∏–∫–∏"));
        cities[11].Places.Add(new PlaceOfInterest("–ú–æ–≥–∏–ª–µ–≤—Å–∫–∞—è –ø–ª–æ—â–∞–¥—å", "–°—Ç–∞—Ä–∏–Ω–Ω–∞—è –ø–ª–æ—â–∞–¥—å –≤ –ú–æ–≥–∏–ª–µ–≤–µ", "–°–æ–ª—è–Ω–∫–∞"));
        cities[12].Places.Add(new PlaceOfInterest("–í–∏—Ç–µ–±—Å–∫–∞—è –ø–ª–æ—â–∞–¥—å", "–ü–ª–æ—â–∞–¥—å –≤ –í–∏—Ç–µ–±—Å–∫–µ", "–î—Ä–∞–Ω–Ω–∏–∫–∏"));
        cities[13].Places.Add(new PlaceOfInterest("–ë—Ä–µ—Å—Ç—Å–∫–∞—è –∫—Ä–µ–ø–æ—Å—Ç—å", "–ò–∑–≤–µ—Å—Ç–Ω–∞—è –∫—Ä–µ–ø–æ—Å—Ç—å –≤ –ë—Ä–µ—Å—Ç–µ", "–ü–µ–ª—å–º–µ–Ω–∏"));
        cities[14].Places.Add(new PlaceOfInterest("–ì—Ä–æ–¥–Ω–µ–Ω—Å–∫–∏–π –ø–∞—Ä–∫", "–ö—Ä–∞—Å–∏–≤—ã–π –ø–∞—Ä–∫ –≤ –ì—Ä–æ–¥–Ω–æ", "–ö–≤–∞—Å"));

        cities[15].Places.Add(new PlaceOfInterest("–ù–∞—Ü–∏–æ–Ω–∞–ª—å–Ω—ã–π –º—É–∑–µ–π –ö–∞–∑–∞—Ö—Å—Ç–∞–Ω–∞", "–ú—É–∑–µ–π –≤ –ê—Å—Ç–∞–Ω–µ", "–ë–µ—à–±–∞—Ä–º–∞–∫"));
        cities[16].Places.Add(new PlaceOfInterest("–°–∞–¥ –ê–ª–º–∞—Ç—ã", "–ü—Ä–∏—Ä–æ–¥–Ω—ã–π —Å–∞–¥ –≤ –ê–ª–º–∞—Ç—ã", "–ú–∞–Ω—Ç—ã"));
        cities[17].Places.Add(new PlaceOfInterest("–¢–æ—Ä–≥–æ–≤—ã–π —Ü–µ–Ω—Ç—Ä –≤ –®—ã–º–∫–µ–Ω—Ç–µ", "–ò–∑–≤–µ—Å—Ç–Ω—ã–π —Ç–æ—Ä–≥–æ–≤—ã–π —Ü–µ–Ω—Ç—Ä", "–®–∞—à–ª—ã–∫"));
        cities[18].Places.Add(new PlaceOfInterest("–ö–∞—Ä–∞–≥–∞–Ω–¥–µ—Ü", "–¢—É—Ä–∏—Å—Ç–∏—á–µ—Å–∫–∞—è –∑–æ–Ω–∞ –≤ –ö–∞—Ä–∞–≥–∞–Ω–¥–µ", "–ñ–∞—Ä–∫–æ–µ"));
        cities[19].Places.Add(new PlaceOfInterest("–ü–∞–≤–ª–æ–¥–∞—Ä—Å–∫–∞—è –Ω–∞–±–µ—Ä–µ–∂–Ω–∞—è", "–ù–∞–±–µ—Ä–µ–∂–Ω–∞—è –≤ –ü–∞–≤–ª–æ–¥–∞—Ä–µ", "–ß–µ–±—É—Ä–µ–∫–∏"));
        cities[20].Places.Add(new PlaceOfInterest("–ê–∫—Ç–æ–±–µ", "–ì–æ—Ä–æ–¥ –≤ –ö–∞–∑–∞—Ö—Å—Ç–∞–Ω–µ", "–ë–µ—à–±–∞—Ä–º–∞–∫"));

        cities[21].Places.Add(new PlaceOfInterest("–¢—è–Ω—å–∞–Ω—å–º—ç–Ω—å", "–ü–ª–æ—â–∞–¥—å –≤ –ü–µ–∫–∏–Ω–µ", "–ü–µ–∫–∏–Ω—Å–∫–∞—è —É—Ç–∫–∞"));
        cities[22].Places.Add(new PlaceOfInterest("–®–∞–Ω—Ö–∞–π—Å–∫–∞—è –Ω–∞–±–µ—Ä–µ–∂–Ω–∞—è", "–ù–∞–±–µ—Ä–µ–∂–Ω–∞—è –≤ –®–∞–Ω—Ö–∞–µ", "–î–∏–º-—Å–∞–º"));
        cities[23].Places.Add(new PlaceOfInterest("–ì—É–∞–Ω—á–∂–æ—É—Å–∫–∏–π —Ö—Ä–∞–º", "–î—Ä–µ–≤–Ω–∏–π —Ö—Ä–∞–º –≤ –ì—É–∞–Ω—á–∂–æ—É", "–õ–∞–ø—à–∞"));
        cities[24].Places.Add(new PlaceOfInterest("–ß—ç–Ω–¥—É –õ–∏ –•—É", "–ü—Ä–∏—Ä–æ–¥–Ω—ã–π –ø–∞—Ä–∫ –≤ –ß—ç–Ω–¥—É", "–•–æ—Ç-–ø–æ—Ç"));
        cities[25].Places.Add(new PlaceOfInterest("–°–∏–∞–Ω—å—Å–∫–∏–π –º—É–∑–µ–π", "–ú—É–∑–µ–π –≤ –°–∏–∞–Ω–µ", "–ú–µ—á—Ç–∞"));
        cities[26].Places.Add(new PlaceOfInterest("–®—ç–Ω—å—á–∂—ç–Ω—å—Å–∫–∏–π –ø–∞—Ä–∫", "–ì–æ—Ä–æ–¥—Å–∫–æ–π –ø–∞—Ä–∫ –≤ –®—ç–Ω—å—á–∂—ç–Ω–µ", "–®–∞—à–ª—ã–∫"));

        return cities;
    }

    public static List<TransportRoute> GetRoutes()
    {
        return new List<TransportRoute>
        {
            // –ü—Ä—è–º—ã–µ —Ä–µ–π—Å—ã
            new TransportRoute("–°–∞–º–æ–ª–µ—Ç", "–ú–æ—Å–∫–≤–∞", "–°–∞–Ω–∫—Ç-–ü–µ—Ç–µ—Ä–±—É—Ä–≥", true, "08:00", "10:00", "4000 —Ä—É–±."),
            new TransportRoute("–ü–æ–µ–∑–¥", "–ú–æ—Å–∫–≤–∞", "–°–∞–Ω–∫—Ç-–ü–µ—Ç–µ—Ä–±—É—Ä–≥", true, "14:00", "18:00", "2000 —Ä—É–±."),
            new TransportRoute("–°–∞–º–æ–ª–µ—Ç", "–ú–æ—Å–∫–≤–∞", "–ù–æ–≤–æ—Å–∏–±–∏—Ä—Å–∫", true, "10:00", "13:00", "5000 —Ä—É–±."),
            new TransportRoute("–ü–æ–µ–∑–¥", "–ï–∫–∞—Ç–µ—Ä–∏–Ω–±—É—Ä–≥", "–ö–∞–∑–∞–Ω—å", true, "12:00", "18:00", "2500 —Ä—É–±."),
            new TransportRoute("–°–∞–º–æ–ª–µ—Ç", "–ú–∏–Ω—Å–∫", "–ü–µ–∫–∏–Ω", true, "13:00", "17:00", "7000 —Ä—É–±."),
            
            // –ü–µ—Ä–µ—Å–∞–¥–∫–∏
            new TransportRoute("–°–∞–º–æ–ª–µ—Ç", "–ú–æ—Å–∫–≤–∞", "–®–∞–Ω—Ö–∞–π", false, "08:00", "18:00", "12000 —Ä—É–±.",
                new List<string> { "–ö–∞–∑–∞–Ω—å", "–ê–ª–º–∞—Ç—ã" }), // –ü–µ—Ä–µ—Å–∞–¥–∫–∞ –≤ –ö–∞–∑–∞–Ω–∏ –∏ –ê–ª–º–∞—Ç—ã
            new TransportRoute("–ü–æ–µ–∑–¥", "–ú–∏–Ω—Å–∫", "–ú–æ—Å–∫–≤–∞", false, "10:00", "22:00", "2500 —Ä—É–±.",
                new List<string> { "–°–∞–Ω–∫—Ç-–ü–µ—Ç–µ—Ä–±—É—Ä–≥" }), // –ü–µ—Ä–µ—Å–∞–¥–∫–∞ –≤ –°–∞–Ω–∫—Ç-–ü–µ—Ç–µ—Ä–±—É—Ä–≥–µ
