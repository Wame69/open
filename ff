        cities[14].Places.Add(new PlaceOfInterest("Гродненский парк", "Красивый парк в Гродно", "Квас"));

        cities[15].Places.Add(new PlaceOfInterest("Национальный музей Казахстана", "Музей в Астане", "Бешбармак"));
