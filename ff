            new TransportRoute("–°–∞–º–æ–ª–µ—Ç", "–®—ã–º–∫–µ–Ω—Ç", "–ê–ª–º–∞—Ç—ã", true, "14:00", "15:00", "2000 —Ä—É–±."),
            new TransportRoute("–ü–æ–µ–∑–¥", "–°–∏–∞–Ω—å", "–ü–µ–∫–∏–Ω", false, "10:00", "20:00", "3500 —Ä—É–±.",
